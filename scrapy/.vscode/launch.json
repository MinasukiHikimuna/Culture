{
    "version": "0.1.0",
    "configurations": [
        {
            "name": "Python: Launch Scrapy Spider",
            "type": "debugpy",
            "request": "launch",
            "module": "scrapy",
            "args": [
                "crawl",
                "lezkiss",
                "--loglevel",
                "DEBUG",
            ],
            "cwd": "${workspaceFolder}/scrapy",
            "console": "integratedTerminal"
        }
    ]
}